{
  "name": "kitsu-wiki",
  "version": "0.1.0",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.1.11",
