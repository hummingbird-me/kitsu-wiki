{
  "name": "kitsu-wiki",
  "version": "0.1.0",
  "dependencies": {
    "@babel/core": "^7.2.0",
    "@reach/router": "^1.3.0-alpha.1",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-links": "^4.1.0-alpha.11",
    "@storybook/addons": "^4.1.0-alpha.11",
    "@storybook/react": "^4.1.0-alpha.11",
    "apollo-boost": "^0.1.22",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.2",
    "graphql": "^14.0.2",
    "graphql-tag.macro": "^2.0.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.15.3",
    "query-string": "^6.2.0",
    "react": "^16.7.0-alpha.2",
    "react-apollo": "^2.3.2",
    "react-apollo-hooks": "^0.2.1",
    "react-dom": "^16.7.0-alpha.2",
    "react-scripts": "2.1.1",
    "reactstrap": "^6.5.0",
    "webpack": "4.19.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "postinstall": "rm -rf ./node_modules/@storybook/react/node_modules/react ./node_modules/@storybook/react/node_modules/react-dom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --jsx-bracket-same-line --end-of-line lf --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
